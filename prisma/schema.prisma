generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model VideoZip {
  uuid         String   @id @default(auto()) @map("_id") @db.ObjectId
  videoUuid    String
  status    String
  path      String?
  images     Image[]
}

model Image {
  uuid         String   @id @default(auto()) @map("_id") @db.ObjectId
  videoZip     VideoZip @relation(fields: [videoZipUuid], references: [uuid])
  videoZipUuid String   @unique @db.ObjectId
  filename     String
  path         String
}
